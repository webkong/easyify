{
  "name": "easyify",
  "version": "1.0.2",
  "description": "easy to work, smart workflow.",
  "main": "index.js",
  "scripts": {
    "deploy:cli": "node build/deploy/cli.deploy.js",
    "deploy": "node build/deploy/index.js",
    "dll": "cross-env NODE_ENV=production webpack --config build/webpack/webpack.dll.conf.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack/webpack.dev.conf.js --type=sigle",
    "dev:multi": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack/webpack.dev.conf.js --type=multi",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack/webpack.prod.conf.js --type=sigle",
    "build:multi": "cross-env NODE_ENV=production webpack --config build/webpack/webpack.prod.conf.js --type=multi",
    "dev:vue": "cross-env NODE_ENV=development VUE=true webpack-dev-server --config build/webpack/webpack.dev.conf.js --type=sigle",
    "dev:vue:multi": "cross-env NODE_ENV=development VUE=true webpack-dev-server --config build/webpack/webpack.dev.conf.js --type=multi",
    "build:vue": "cross-env NODE_ENV=production VUE=true webpack --config build/webpack/webpack.prod.conf.js --type=sigle",
    "build:vue:multi": "cross-env NODE_ENV=production VUE=true webpack --config build/webpack/webpack.prod.conf.js --type=multi"
  },
  "keywords": [
    "easyify",
    "webpack",
    "workflow",
    "Vue",
    "H5"
  ],
  "author": "wangsw@ushareit.com",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.3",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.12.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "npm": "^6.2.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "ora": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-smart-import": "^0.7.6",
    "precss": "^3.1.2",
    "sass": "^1.7.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vconsole": "^3.2.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "vuepress": "^0.12.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element": "^0.1.4",
